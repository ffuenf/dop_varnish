<% if node['varnish']['version'] == "4.0" || node['varnish']['version'] == "4.1" || node['varnish']['version'] == "5.0" %>
vcl 4.0;
<% end %>
import std;
include ".all_includes.vcl";
backend default {
  .host = "<%= node['varnish']['backend_host'] %>";
  .port = "<%= node['varnish']['backend_port'] %>";
}